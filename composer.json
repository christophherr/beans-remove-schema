{
  "name": "beans-remove-schema",
  "description": "WordPress plugin removing the built-in Schema.org markup from the Beans framework.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0+",
  "homepage": "http://www.christophherr.com/",
  "support": {
    "issues": "https://github.com/christophherr/beans-remove-schema/issues",
    "source": "https://github.com/christophherr/beans-remove-schema"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "require": {
    "php": "^5.6|^7",
    "composer/installers": "^1.4"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
    "roave/security-advisories": "dev-master",
    "sirbrillig/phpcs-variable-analysis": "^2.0",
    "wimg/php-compatibility": "^8.0",
    "wp-coding-standards/wpcs": "^0.14.1"
  },
  "config": {
    "sort-order": true
  },
  "scripts": {
    "phpcs": "phpcs --runtime-set testVersion 5.6"
  }
}
